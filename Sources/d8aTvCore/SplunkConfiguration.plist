<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>NSBonjourServices</key>
	<array>
		<string>d8atv_tcp</string>
	</array>
	<key>NSLocalNetworkUsageDescription</key>
	<string>Exchange data with nearby devices running the d8aTv.</string>
	<key>baseURL</key>
	<string>https://localhost:8089</string>
	<key>defaultApp</key>
	<string>search</string>
	<key>defaultOwner</key>
	<string>admin</string>
	<key>timeout</key>
	<real>30</real>
	<key>maxRetries</key>
	<integer>3</integer>
	<key>retryDelay</key>
	<real>2</real>
	<key>allowInsecureConnections</key>
	<true/>
	<key>validateSSLCertificate</key>
	<true/>
	<key>dashboardSyncSettings</key>
	<dict>
		<key>batchSize</key>
		<integer>50</integer>
		<key>maxDashboards</key>
		<integer>1000</integer>
		<key>includePrivate</key>
		<false/>
		<key>defaultAppsToSync</key>
		<array>
			<string>search</string>
			<string>SplunkEnterpriseSecuritySuite</string>
			<string>Splunk_TA_nix</string>
		</array>
		<key>excludeSystemApps</key>
		<true/>
	</dict>
	<key>appFilters</key>
	<dict>
		<key>excludePatterns</key>
		<array>
			<string>^sample_</string>
			<string>^learned$</string>
			<string>^legacy$</string>
		</array>
		<key>includeOnlyVisible</key>
		<true/>
		<key>includeOnlyEnabled</key>
		<true/>
	</dict>
	<key>authenticationMethods</key>
	<dict>
		<key>preferredMethod</key>
		<string>token</string>
		<key>supportedMethods</key>
		<array>
			<string>token</string>
			<string>basic</string>
			<string>sessionKey</string>
		</array>
		<key>tokenEndpoint</key>
		<string>/services/auth/login</string>
		<key>sessionKeyEndpoint</key>
		<string>/services/auth/login</string>
	</dict>
	<key>coreDataMapping</key>
	<dict>
		<key>entityName</key>
		<string>SplunkDashboard</string>
		<key>batchInsertSize</key>
		<integer>100</integer>
		<key>preserveSourceXML</key>
		<true/>
		<key>extractTokens</key>
		<true/>
		<key>parseSearches</key>
		<true/>
	</dict>
	<key>debugSettings</key>
	<dict>
		<key>logLevel</key>
		<string>info</string>
		<key>logNetworkRequests</key>
		<true/>
		<key>logCoreDataOperations</key>
		<false/>
		<key>enablePerformanceMetrics</key>
		<true/>
	</dict>
</dict>
</plist>
